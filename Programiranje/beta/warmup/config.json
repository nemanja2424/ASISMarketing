  "language_settings": {
    "primary_language": "sr",
    "message_language": "serbian",
    "use_emojis": true,
    "emoji_probability": 0.7
  },
  
  "logging_settings": {
    "log_level": "INFO",
    "log_to_file": true,
    "log_file": "warmup/logs/warmup.log",
    "log_to_console": true,
    "verbose_logging": false
  },
  
  "safety_limits": {
    "max_actions_per_hour": 200,
    "max_follows_per_day": 150,
    "max_unfollows_per_day": 100,
    "max_dms_per_day": 50,
    "min_delay_between_same_action_sec": 3,
    "rest_period_after_batch_min": 60
